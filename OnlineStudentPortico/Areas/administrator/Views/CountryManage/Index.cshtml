@model IEnumerable<OnlineStudentPortico.Models.CountryMaster>

@{
    ViewBag.Title = "Country Manage";
    Layout = "~/Areas/administrator/Views/Shared/_adminLayout.cshtml";
}


<div id="page-wrapper">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">@ViewBag.Title</h1>
            </div>
            <!-- /.col-lg-12 -->
        </div>
        <!-- /.row -->
        <div class="row">
            <table class="table">
                <tr>
                    <td colspan="2">


                        @Html.Partial("_AddCountry")



                    </td>
                </tr>
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.CountyName)
                    </th>
                    <th></th>
                </tr>

                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.CountyName)
                        </td>
                        <td>
                            @Html.ActionLink("Edit", "Edit", new { id = item.CountryId }) |
                            @Html.ActionLink("Details", "Details", new { id = item.CountryId }) |
                            @Html.ActionLink("Delete", "Delete", new { id = item.CountryId })
                        </td>
                    </tr>
                }

            </table>
        </div>
        <!-- /.row -->
        <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
</div>


